{
  "name": "php-fn/di-test-manual",
  "license": "MIT",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "package",
      "package": {
        "type": "composer-plugin",
        "name": "php-fn/di",
        "version": "999",
        "dist": {
          "type": "path",
          "url": "../../../"
        },
        "require": {
          "php-di/php-di": "^6.0",
          "composer-plugin-api": "^1.0"
        },
        "autoload": {
          "psr-4": {"fn\\": ["src/fn"]}
        },
        "extra": {
          "class": "fn\\Composer\\DIPlugin"
        }
      }
    }
  ],
  "require": {
    "php-fn/di": "999"
  },
  "config": {
    "vendor-dir": "/tmp/vendor-di-test-manual/"
  },
  "extra": {
    "di": {
      "@fn\\c1": "config/c1.php",
      "@fn\\c2": {
        "@fn\\c3": {
          "c3": "C3",
          "@fn\\c31": {"c31": "C31"}
        }
      },
      "foo": "bar"
    }
  }
}
