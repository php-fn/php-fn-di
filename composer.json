{
  "name": "php-fn/di",
  "description": "composer plugin for dependency injection containers",
  "type": "composer-plugin",
  "license": "MIT",
  "homepage": "https://github.com/php-fn/php-fn-di",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "php-fn/php-fn": "^1",
    "php-fn/invoker": "^2",
    "php-di/php-di": "^6",
    "composer-plugin-api": "^1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "docker-xampp/bin": "^1",
    "php-fn/test": "^1",
    "composer/composer": "^1"
  },
  "autoload": {
    "psr-4": {
      "fn\\": ["src/fn"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "fn\\": ["tests/fn"]
    }
  },
  "extra": {
    "class": "fn\\Composer\\DIPlugin"
  }
}
