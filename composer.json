{
  "name": "php-fn/di",
  "description": "composer plugin for dependency injection containers",
  "type": "composer-plugin",
  "license": "MIT",
  "homepage": "https://github.com/php-fn/php-fn-di",
  "require": {
    "php": ">=7.1",
    "php-fn/php-fn": "^3.0",
    "php-fn/invoker": "^2.0",
    "php-di/php-di": "^6.0",
    "composer-plugin-api": "^1"
  },
  "require-dev": {
    "docker-xampp/bin": "^1",
    "php-coveralls/php-coveralls": "^2.1",
    "php-fn/test": "^3.0",
    "doctrine/annotations": "^1",
    "composer/composer": "^1",
    "ext-apcu": "*",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "php\\": ["src/php"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "php\\": ["tests/php"]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    },
    "class": "php\\Composer\\DIPlugin"
  },
  "scripts": {
    "test": "php -d error_reporting=22527 vendor/bin/phpunit tests/"
  }
}
